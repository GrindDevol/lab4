<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:tools="http://schemas.android.com/tools" xmlns:android="http://schemas.android.com/apk/res/android" tools:ignore="ExtraText,MissingDefaultResource">
    <item android:state_pressed="false">
        <shape android:shape="rectangle">
            <corners android:radius="12dp"/>

            <solid android:color="#008080"/>

            <stroke android:width="2dip"
                android:color="#668B8B"/>

        </shape>
    </item>
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <corners android:radius="12dp"/>

            <solid android:color="#009090"/>

            <stroke android:width="4dip"
                android:color="#009090"/>

        </shape>
    </item>
</selector>
